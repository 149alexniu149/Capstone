<p id="notice"><%= notice %></p>

<h1>Listing Bitstamps</h1>

<table>
  <thead>
    <tr>
      <th>Timestamp</th>
      <th>Avgprice</th>
      <th>Bid</th>
      <th>Ask</th>
      <th>Volume</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @bitstamps.each do |bitstamp| %>
      <tr>
        <td><%= bitstamp.timestamp %></td>
        <td><%= bitstamp.avgprice %></td>
        <td><%= bitstamp.bid %></td>
        <td><%= bitstamp.ask %></td>
        <td><%= bitstamp.volume %></td>
        <td><%= link_to 'Show', bitstamp %></td>
        <td><%= link_to 'Edit', edit_bitstamp_path(bitstamp) %></td>
        <td><%= link_to 'Destroy', bitstamp, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Bitstamp', new_bitstamp_path %>
