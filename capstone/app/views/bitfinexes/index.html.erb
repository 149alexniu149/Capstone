<p id="notice"><%= notice %></p>

<h1>Listing Bitfinexes</h1>

<table>
  <thead>
    <tr>
      <th>Timestamp</th>
      <th>Avgprice</th>
      <th>Bid</th>
      <th>Ask</th>
      <th>Volume</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @bitfinexes.each do |bitfinex| %>
      <tr>
        <td><%= bitfinex.timestamp %></td>
        <td><%= bitfinex.avgprice %></td>
        <td><%= bitfinex.bid %></td>
        <td><%= bitfinex.ask %></td>
        <td><%= bitfinex.volume %></td>
        <td><%= link_to 'Show', bitfinex %></td>
        <td><%= link_to 'Edit', edit_bitfinex_path(bitfinex) %></td>
        <td><%= link_to 'Destroy', bitfinex, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Bitfinex', new_bitfinex_path %>
